		 -->
	 
		 <action name="ThemeAction" class="ThemeAction"></action>
		 <action name="TopicAction" class="TopicAction"></action>
		 <action name="TopicCommentAction" class="TopicCommentAction"></action>
		
	</package>
	
</struts>